	!p³yta-k¹towniki
	WPCSYS
	WPOFFS, bk_bok/2+uch_kt_szer-uch_kt_otw_pszer, -bk_bok/2-bk_odl_od_pl+uch_kt_gr, uch_kt_otwd_pdlu
