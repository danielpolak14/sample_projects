!Tworzenie œrub, ³¹cz¹cych k¹towniki z belk¹ i p³yt¹.
!*****parametry*****

!*****tworzenie modeli*****
	CSYS, 21
	!belka-k¹towniki
	WPCSYS

	WPOFFS, bk_bok/2+uch_kt_gr, 0, uch_kt_otwg_pdlu
	WPROTA, 0, 0, -90

	*DO, ind, 1, 3, 1
		WPROTA, 45, 0, 0
  		*USE, utworz_srube_volu, 26000+100*ind, srub_lbnk_sr, srub_lbnk_wys, srub_sr, bk_bok+2*uch_kt_gr, sr_obciazenie_wstepne, 2, 2, 2, 20
  		WPROTA, -45, 0, 0
		WPOFFS, uch_kt_otwg_roz, 0, 0
