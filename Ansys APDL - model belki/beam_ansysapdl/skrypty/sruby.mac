		WPOFFS, -1*(2*(uch_kt_szer-uch_kt_otw_pszer)+bk_bok)*ind, 0, 0
		*DO, ind2, 0, 1, 1
			WPOFFS, 0, uch_kt_otwd_roz*(ind2*(1-ind)+ind2*(-1)*ind), 0
			WPROTA, 45, 0, 0
			*USE, utworz_srube_volu, 26500+200*ind+100*ind2, srub_lbnk_sr, srub_lbnk_wys, srub_sr, uch_pl_wys+uch_kt_gr, sr_obciazenie_wstepne, 2, 2, 2, 20
			WPROTA, -45, 0, 0
		*ENDDO
	*ENDDO
	
	WPCSYS